<?xml version="1.0" encoding="UTF-8"?>
<mapred>
    <!-- profile -->
    <baseId>fcb988d02a8e1ee619dc4122427a052e</baseId>
    <projectId>2420</projectId>
    <resourceName>connect_feature1</resourceName>
    <idePath>工作流/mapreduce</idePath>

    <!-- classes -->
    <jobLauncher>my.group.connect_feature1.JobLauncher</jobLauncher>
    <mapper>my.group.connect_feature1.MyMapper</mapper>
    <reducer>my.group.connect_feature1.MyReducer</reducer>
    <combiner>my.group.connect_feature1.MyCombiner</combiner>

    <!--task-->
    <mapOutputKey>word:string</mapOutputKey>
    <mapOutputValue>cnt:bigint</mapOutputValue>
    <!--
    <partitionColumns>col1,col2</partitionColumns>
    <outputKeySortColumns>col1,col2</outputKeySortColumns>
    <outputKeySortOrders>ASC,DESC</outputKeySortOrders>
    <outputGroupingColumns>col1,col2</outputGroupingColumns>
    <numReduceTask>8</numReduceTask>
    <memoryForMapTask>2048</memoryForMapTask>
    <memoryForReduceTask>2048</memoryForReduceTask>
    -->

    <!-- tables -->
    <inputTables>
        <table>
            <name>wordcount_in</name>
            <partitions>
                <partition>dt={yyyymmdd}</partition>
                <partition>dt={yyyymmdd-1}</partition>
            </partitions>
        </table>
    </inputTables>
    <outputTable>
        <name>wordcount_out</name>
        <partition>dt={yyyymmdd}</partition>
    </outputTable>
</mapred>
